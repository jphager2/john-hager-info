<%= form_for(@client) do |f| %>
  <div class="row">
    <div class="small-12 columns">
      <%= f.text_field :name %>
    </div>
  </div>
  <div class="row">
    <div class="small-12 columns">
      <%= f.text_field :code %>
    </div>
  </div>
  <div class="row">
    <div class="small-12 columns">
      <%= f.text_field :contact_name %>
    </div>
  </div>
  <div class="row">
    <div class="small-12 columns">
      <%= f.text_field :contact_email %>
    </div>
  </div>
  <div class="row">
    <div class="small-12 columns">
      <%= f.text_field :contact_phone %>
    </div>
  </div>
  <div class="row">
    <div class="small-12 columns">
      <%= f.text_field :address1 %>
      <%= f.text_field :address2 %>
    </div>
    <div class="small-4 columns">
      <%= f.text_field :city %>
    </div>
    <div class="small-2 columns">
      <%= f.text_field :state %>
    </div>
    <div class="small-2 columns">
      <%= f.text_field :zip %>
    </div>
    <div class="small-4 columns">
      <%= f.text_field :country %>
    </div>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
