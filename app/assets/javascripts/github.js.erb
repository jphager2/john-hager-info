var scheme   = "ws://";
var uri      = scheme + window.document.location.host + "/";
var ws       = new WebSocket(uri);

ws.onmessage = function(message) {
  var data = JSON.parse(message.data);
  var eventsData = (data.events);
  console.log(eventsData);
  html = "" 
  for (i in eventsData) {
    evnt = eventsData[i];

    html += '<tr><td class="code">';
    html += '<a href="' + evnt.uri +'" target="_blank">';
    html += '<div>';
    html += '<span>' + evnt.datetime + ', </span>';
    html += '<span class="red">John</span>: <span class="yellow">'; 
    html += evnt.action; 
    html += '</span>&nbsp;=><br><span class="green">';
    html += evnt.repo;
    html += '</span><br>';
    html += '<blockquote>' + evnt.message + '</blockquote>'
    html += '</div>';
    html += '</td></tr>';
  }

  $("#githubActivity").html(html);
};


